# Expected:
# *
# **
# ***
# ****
# *****

fun main(): int = {
    i = 0;
    c = '*';

    while i < 5 do {
        j = i + 1;

        while j > 0 do {
            putChar((c : int));
            j = j - 1;
        };

        putChar(10);
        i = i + 1;
    };
    0;
} where {
    var i: int
    var j: int
    var c: char
}

fun putChar(c : int) : void
